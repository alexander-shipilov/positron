{
  "name": "positron-dom",
  "version": "0.1.0",
  "description": "Dom utils",
  "main": "./index.js",
  "dependencies": {
    "positron-core": "^0.1.0",
    "positron-immutable": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "flow-bin": "^0.59.0",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build": "npm run build-lib && npm run build-index",
    "build-lib": "rimraf lib && babel ./src --out-dir ./lib --ignore __mocks__,__tests__",
    "build-index": "rimraf index.js && babel-node ../make-index.js ./src --root ./lib --out-file ./index.js",
    "eslint": "eslint ./src",
    "flow": "flow",
    "prepublish": "npm run eslint && npm run test && npm run build",
    "test": "jest --no-cache --runInBand",
    "test-watch": "npm run test -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexander-shipilov/positron.git"
  },
  "keywords": [
    "positron-ui",
    "positron-core"
  ],
  "author": "Alexander Shipilov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexander-shipilov/positron/issues"
  },
  "homepage": "https://github.com/alexander-shipilov/positron#readme",
  "false": {}
}
