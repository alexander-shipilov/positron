{
  "name": "positron-core",
  "version": "0.0.39",
  "description": "Core library for positron-ui",
  "main": "./build/index.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "intl": "^1.2.5",
    "intl-format-cache": "^2.0.5",
    "intl-messageformat": "^2.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "whatwg-fetch": "^2.0.3"
  },
  "peerDependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.52.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "build": "npm run clean && npm run build-js && npm run build-copy-package-json ",
    "build-copy-package-json": "babel-node tools/copy-package-json build",
    "build-js": "babel ./src --out-dir ./build --ignore spec.js,test.js",
    "clean": "rimraf build",
    "eslint": "eslint ./src",
    "flow": "flow",
    "publish": "npm run clean && npm run eslint && npm run test && npm run build && cd build && npm publish",
    "test": "jest --no-cache --runInBand",
    "test-coverage": "npm run  test -- --coverage",
    "test-watch": "npm run test -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexander-shipilov/positron-core.git"
  },
  "keywords": [
    "positron-ui",
    "positron-core"
  ],
